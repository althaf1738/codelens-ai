# Provider selection (required)
# Choose one embedding provider: gemini | openai
EMBEDDING_PROVIDER=gemini
# Choose one LLM provider: gemini | openai | anthropic
LLM_PROVIDER=gemini

# Gemini
GOOGLE_API_KEY=your_gemini_key
GEMINI_EMBED_MODEL=models/gemini-embedding-001
GEMINI_GEN_MODELS=gemini-3-flash-preview,gemini-3-flash-preview-latest

# OpenAI
OPENAI_API_KEY=your_openai_key
OPENAI_EMBED_MODEL=text-embedding-3-small
OPENAI_LLM_MODEL=gpt-4o-mini

# Anthropic (Claude)
ANTHROPIC_API_KEY=your_anthropic_api_key
CLAUDE_MODEL=claude-3-5-sonnet-latest

# Storage / vector DB
# Point to your Qdrant instance; change host/port per environment. Unset to disable vector storage.
QDRANT_URL=http://localhost:6333


STORAGE_DIR=.data/python-backend
# Optional: override SQLite DB path (default: STORAGE_DIR/projects.db)
# DATABASE_PATH=.data/python-backend/projects.db
